 <%= semantic_form_for :line_item, :url => item_list_line_items_path do |form| %>
    <%= form.inputs :name, :quantity %>
    <%= form.input :note, :required => false %>
    <%= form.input :cost, :required => false %>
    <%= form.input :status, :required => false, :collection => ["Unknown", "Buy Order", "Purchased", "Delivered"] %>
    <%= form.buttons %>
  <% end %>